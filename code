
void	ft_sort_params(int ac, char *av[])
{
	char	*temp;
	int		i;

	i = 1;
	while (i < ac - 1)
	{
		if (ft_strcmp(av[i], av[i + 1]) > 0)
		{
			temp = av[i];
			av[i] = av[i + 1];
			av[i + 1] = temp;
			i = 0;
		}
		i = i + 1;
	}
}

int	main(int ac, char *av[])
{
	int	i;

	i = 1;
	ft_sort_params(ac, av);
	while (i < ac)
	{
		ft_putstr(av[i]);
		i++;
	}
	return (0);
}
